    "prettier": "^2.7.1",
    "react-app-rewired": "^2.2.1"
  },
  "lint-staged": {
    "*.{js,md,json}": "prettier --write",
    "*.js": "eslint --fix",
    "*.py": [
      "yapf -i",
      "isort"
    ]
  }
